#include <stdio.h>
#include <string.h>
#include <stdbool.h>
int main()
{
    char s[100];
    scanf("%s",&s);
   
   int i=0,n=strlen(s);
   bool flag= true;
   while (s[i]!='\0')
   {
      if(s[i]=='.'){
         flag = false;
          if(s[n-1]=='.'){                 
          printf("invalid");
          break;
          } 

          printf("valid");
          break;
   
      }

    i++;
    
    
   }
   if(flag==true) 
   {
     printf("invalid");
   }
   
}
